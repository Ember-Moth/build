{
  "name": "my-qwik-empty-starter",
  "private": true,
  "description": "Blank project with routing included",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages deploy ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "drizzle:check": "drizzle-kit check",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:pull": "drizzle-kit pull",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:up": "drizzle-kit up",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "openapi": "openapi2ts",
    "postinstall": "drizzle-kit generate",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist --compatibility-flags=nodejs_als",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.13.0",
    "@builder.io/qwik-city": "^1.13.0",
    "@cloudflare/workers-types": "^4.20250407.0",
    "@modular-forms/qwik": "^0.29.1",
    "@octokit/rest": "^21.1.1",
    "@qwik-ui/headless": "^0.6.6",
    "@qwik-ui/utils": "^0.3.1",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint": "8.56.10",
    "@types/node": "20.14.11",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@umijs/openapi": "^1.13.0",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.8.1",
    "class-variance-authority": "^0.7.1",
    "crypto-js": "^4.2.0",
    "drizzle-kit": "^0.30.3",
    "drizzle-orm": "^0.39.0",
    "drizzle-valibot": "^0.4.1",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "^1.13.0",
    "prettier": "3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "qwik-sonner": "^1.0.3",
    "tailwindcss": "^4.0.0",
    "tw-animate-css": "^1.2.5",
    "typescript": "5.4.5",
    "undici": "*",
    "universal-cookie": "^8.0.1",
    "valibot": "^1.0.0",
    "vite": "5.3.5",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime"
}
